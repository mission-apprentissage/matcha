{
  "name": "mna-matcha",
  "version": "0.0.1",
  "description": "[MNA] Matcha",
  "repository": "https://github.com/mission-apprentissage/matcha-template.git",
  "private": false,
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon --ignore tests/ src/index.js",
    "debug": "nodemon --inspect=0.0.0.0 --signal SIGINT --ignore tests/ src/index.js",
    "test": "mocha --recursive --exit --timeout 5000 tests/",
    "lint": "eslint src/tests",
    "docker:start": "docker-compose up -d --build --force-recreate",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose kill && docker system prune --force --volumes",
    "form:dummy": "node src/jobs/seed/createFormulaire.js",
    "form:import": "node src/jobs/formulaire/import.js",
    "form:campagne": "node src/jobs/formulaire/campagne.js",
    "form:index": "node src/jobs/formulaire/generateEsIndex.js",
    "clear": "node src/jobs/clear/index.js"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.9.1",
    "axios": "0.20.0",
    "body-parser": "1.19.0",
    "boom": "7.3.0",
    "bunyan": "1.8.12",
    "bunyan-mongodb-stream": "0.0.3",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "compose-middleware": "5.0.1",
    "config": "3.3.1",
    "elasticsearch-scroll-stream": "1.3.2",
    "env-var": "6.1.1",
    "express": "4.17.1",
    "fs-extra": "9.0.0",
    "generate-password": "1.5.1",
    "iconv-lite": "0.6.2",
    "joi": "17.2.1",
    "json2csv": "5.0.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "moment": "2.26.0",
    "mongoose": "5.10.2",
    "multipipe": "4.0.0",
    "nanoid": "3.1.22",
    "node-cron": "3.0.0",
    "passport": "0.4.1",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "requestretry": "5.0.0",
    "sha512crypt-node": "0.1.0",
    "xlsx": "0.16.1"
  },
  "engines": {
    "node": ">=12.11.0"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "axiosist": "0.9.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "mocha": "7.1.2",
    "nodemon": "2.0.3",
    "prettier": "2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/preventSensibleFilesCommit.sh && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
